<table>
    <tbody>
        <% @books.each do |book| %>
        <tr>
            <td><%= book.isbn%></td>
            <td><%= book.title%></td>
            <td><%= book.price%></td>
            <td><%= book.publisher%></td>
            <td><%= book.published%></td>
            <td><%= book.dl%></td>
        </tr>
        <%end%>
    </tbody>
</table>