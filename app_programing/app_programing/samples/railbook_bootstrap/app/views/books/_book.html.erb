<div id="<%= dom_id book %>">
  <p>
    <strong>Isbn:</strong>
    <%= book.isbn %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= book.price %>
  </p>

  <p>
    <strong>Publisher:</strong>
    <%= book.publisher %>
  </p>

  <p>
    <strong>Published:</strong>
    <%= book.published %>
  </p>

  <p>
    <strong>Dl:</strong>
    <%= book.dl %>
  </p>

</div>
